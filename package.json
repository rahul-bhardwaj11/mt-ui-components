{
  "name": "@mindtickle/mt-ui-components",
  "version": "0.0.13-0",
  "description": "Collection of common React UI components for Mindtickle",
  "repository": "git@github.com:MindTickle/mt-ui-components.git",
  "author": "Mindtickle",
  "license": "MIT",
  "scripts": {
    "start": "start-storybook -p 6006",
    "test": "jest --verbose ",
    "test:update": "jest -u",
    "prebuild": "rimraf ./lib",
    "build:dev": "babel src --out-dir lib --copy-files --ignore test,*.stories.js --source-maps true && npm run lint:watch",
    "build:prod": "babel src --out-dir lib --copy-files --ignore test,*.stories.js --source-maps true && npm run test && npm run lint",
    "build:watch": "npm run build:dev -- -w",
    "postbuild": "node utils/generateModulesList.js",
    "deploy:sb": "npm run build-storybook && gh-pages -m 'update storybook' -d ./build",
    "deploy": "sh ./deploy.sh",
    "build-storybook": "build-storybook -o ./build",
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --write",
    "lint": "./node_modules/.bin/eslint src/**",
    "lint:watch": "./node_modules/.bin/esw src/** -w"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "devDependencies": {
    "@storybook/addon-info": "^3.4.8",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addon-options": "^3.4.8",
    "@storybook/cli": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^22.4.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-react-require": "^1.0.1",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-plugin-transform-require-ignore": "^0.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.22.0",
    "cross-env": "^3.1.4",
    "css-loader": "0.23.1",
    "css-modules-require-hook": "^4.0.5",
    "debug": "^2.6.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-watch": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.10.1",
    "gh-pages": "^0.12.0",
    "husky": "^0.14.3",
    "is-plain-object": "^2.0.4",
    "jest": "^22.4.4",
    "jest-css-modules": "^1.1.0",
    "jsdom": "^9.9.1",
    "lint-staged": "^5.0.0",
    "node-sass": "^4.9.0",
    "node-sass-utils": "^1.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.7",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.4.1",
    "react-hot-loader": "3.0.0-beta.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.8",
    "webpack": "^3.12.0",
    "wix-storybook-utils": "^1.0.85"
  },
  "pre-commit": "precommit",
  "lint-staged": {
    "src/{,**/}*.{js,jsx,json,css,scss}": [
      "prettier --write",
      "git add"
    ],
    "src/{,**/}*.{js,jsx}": [
      "eslint"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "testRegex": "^.+\\.test\\.js?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
  "dependencies": {
    "antd": "^3.6.5",
    "classnames": "^2.2.6",
    "identity-obj-proxy": "^3.0.0",
    "install": "^0.11.2",
    "npm": "^6.1.0",
    "prop-types": "^15.6.2",
    "react": "16.2.0",
    "react-icons": "^2.2.3",
    "react-sortable-tree": "^2.2.0",
    "react-test-renderer": "^16.1.1",
    "react-tether": "^0.5.7",
    "styled-components": "^3.3.3"
  },
  "peerDependencies": {
    "react": "^16.1.0",
    "antd": "^3.5.2",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "styled-components": "^3.3.0",
    "react-sortable-tree": "^2.1.2"
  },
  "publishConfig": {
    "registry": "http://verdaccio.ops.mindtickle.com"
  }
}
