{
  "name": "@mindtickle/mt-ui-components",
  "version": "0.0.9",
  "description": "Collection of common React UI components for Mindtickle",
  "repository": "git@github.com:MindTickle/mt-ui-components.git",
  "author": "Mindtickle",
  "license": "MIT",
  "scripts": {
    "start": "npm run build:watch & start-storybook -p 6006",
    "test": "NODE_ENV=test jest",
    "prebuild": "rimraf ./lib",
    "build": "babel src/components --out-dir lib --copy-files --ignore test,*.stories.js --source-maps true",
    "build:watch": "npm run build -- -w",
    "postbuild": "node utils/generateModulesList.js",
    "deploy:sb": "npm run build-storybook && gh-pages -m 'update storybook' -d ./build",
    "deploy": "sh ./deploy.sh",
    "build-storybook": "build-storybook -o ./build",
    "prettier": "prettier --single-quote --write"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^22.0.6",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-react-require": "^1.0.1",
    "babel-plugin-transform-require-ignore": "^0.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.13.0",
    "babel-runtime": "^6.22.0",
    "cross-env": "^3.1.4",
    "css-loader": "0.23.1",
    "css-modules-require-hook": "^4.0.5",
    "debug": "^2.6.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "^0.10.1",
    "gh-pages": "^0.12.0",
    "jest": "^22.0.4",
    "jsdom": "^9.9.1",
    "lint-staged": "^5.0.0",
    "node-sass": "^4.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.10.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.1.0",
    "react-hot-loader": "3.0.0-beta.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.8",
    "@storybook/addon-info": "^3.3.11",
    "@storybook/addon-knobs": "^3.4.5",
    "@storybook/addon-links": "^3.3.11",
    "@storybook/addon-options": "^3.3.11",
    "@storybook/cli": "^3.3.11",
    "@storybook/react": "^3.3.11",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "eslint-plugin-babel": "^5.1.0",
    "extract-text-webpack-plugin": "^3.0.2"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "jest": {
    "testRegex": "^.+\\.test\\.js?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
  "dependencies": {
    "install": "^0.11.0",
    "npm": "^6.0.1",
    "react-icons": "^2.2.3",
    "react-test-renderer": "^16.1.1",
    "react-tether": "^0.5.7"
  },
  "peerDependencies":{
    "react": "^16.1.0",
    "antd": "^3.5.2",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "styled-components": "^3.3.0",
    "react-sortable-tree": "^2.1.2"
  },
  "publishConfig": {
    "registry": "http://verdaccio.ops.mindtickle.com"
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier",
      "npm run lint:fix",
      "git add"
    ]
  }
}
